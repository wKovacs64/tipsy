{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "esModuleInterop": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [".cache", "cypress", "public"]
}
