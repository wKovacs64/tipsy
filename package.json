    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "0.0.0",
    "gatsby-plugin-typescript": "2.0.3",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.1",
    "rimraf": "2.6.3",
    "start-server-and-test": "1.7.11",
    "typescript": "3.2.4"
  }
