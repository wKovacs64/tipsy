{
  "name": "tipsy",
  "version": "1.2.3",
  "description": "A simple tip calculator and bill divider PWA",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/tipsy.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/tipsy/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "preserve": "yarn build",
    "serve": "gatsby serve --port 8000",
    "test": "yarn test:e2e:ci",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test serve 8000 cy:run",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@quid/merge-refs": "4.0.0",
    "@wkovacs64/normalize.css": "8.0.1",
    "currency.js": "2.0.3",
    "gatsby": "2.24.75",
    "gatsby-plugin-emotion": "4.3.13",
    "gatsby-plugin-manifest": "2.4.34",
    "gatsby-plugin-netlify": "2.3.17",
    "gatsby-plugin-offline": "3.2.31",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-typescript": "2.4.21",
    "gatsby-plugin-typography": "2.5.13",
    "gatsby-plugin-use-dark-mode": "1.1.2",
    "git-current-commit": "1.1.0",
    "hex-rgb": "4.2.0",
    "lodash": "4.17.20",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-icons": "3.11.0",
    "react-switch": "5.0.1",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-moraga": "0.16.19",
    "use-dark-mode": "2.3.1",
    "use-persisted-state": "0.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/git": "9.0.0",
    "@testing-library/cypress": "7.0.1",
    "@types/cypress-axe": "0.8.0",
    "@types/hex-rgb": "4.1.1",
    "@types/lodash": "4.14.162",
    "@types/node": "12.12.67",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/typography": "0.16.3",
    "@types/use-persisted-state": "0.3.0",
    "@wkovacs64/prettier-config": "3.0.0",
    "axe-core": "4.0.2",
    "babel-eslint": "10.1.0",
    "commitizen": "4.2.1",
    "cypress": "5.3.0",
    "cypress-axe": "0.8.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.11.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-wkovacs64": "9.0.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "semantic-release": "17.2.1",
    "start-server-and-test": "1.11.5",
    "typescript": "4.0.3"
  },
  "resolutions": {
    "**/@types/react": "16.9.52",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1"
  }
}
