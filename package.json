  },
  "scripts": {
    "build": "gatsby build",
    "check-types": "tsc",
    "clean": "rimraf .cache public",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md,yml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "serve": "gatsby serve --port 8000",
    "test": "yarn test:e2e:ci",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.6",
    "@emotion/styled": "10.0.6",
    "@wkovacs64/normalize.css": "8.0.1",
    "gatsby": "2.0.98",
    "gatsby-plugin-emotion": "4.0.1",
    "gatsby-plugin-manifest": "2.0.13",
    "gatsby-plugin-netlify": "2.0.7",
    "gatsby-plugin-offline": "2.0.21",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-typography": "2.2.5",
    "git-current-commit": "1.1.0",
    "prop-types": "15.6.2",
    "react": "16.8.0-alpha.1",
    "react-dom": "16.8.0-alpha.1",
    "react-helmet": "6.0.0-beta",
    "react-icons": "3.3.0",
    "react-typography": "0.16.18",
    "react-use": "5.2.1",
    "typography": "0.16.18",
    "typography-theme-moraga": "0.16.18"
