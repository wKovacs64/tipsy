{
  "name": "tipsy",
  "version": "0.1.0-dev.0",
  "description": "A simple tip calculator and bill divider PWA",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/tipsy.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/tipsy/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "check-types": "tsc",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md,yml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "preserve": "yarn build",
    "serve": "gatsby serve --port 8000",
    "test": "yarn test:e2e:ci",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test serve 8000 cy:run",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "@wkovacs64/normalize.css": "8.0.1",
    "currency.js": "1.2.1",
    "gatsby": "2.1.8",
    "gatsby-plugin-emotion": "4.0.3",
    "gatsby-plugin-manifest": "2.0.18",
    "gatsby-plugin-netlify": "2.0.11",
    "gatsby-plugin-offline": "2.0.24",
    "gatsby-plugin-react-helmet": "3.0.6",
    "gatsby-plugin-typescript": "2.0.8",
    "gatsby-plugin-typography": "2.2.7",
    "git-current-commit": "1.1.0",
    "hex-rgb": "4.0.0",
    "lodash": "4.17.11",
    "prop-types": "15.7.2",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-helmet": "6.0.0-beta",
    "react-icons": "3.4.0",
    "react-switch": "4.1.0",
    "react-typography": "0.16.18",
    "terser": "3.16.1",
    "typography": "0.16.18",
    "typography-theme-moraga": "0.16.18",
    "use-dark-mode": "2.3.0",
    "use-persisted-state": "0.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.3",
    "@types/cypress-axe": "0.3.0",
    "@types/hex-rgb": "3.0.0",
    "@types/node": "10.12.26",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "@types/react-helmet": "5.0.8",
    "@types/typography": "0.16.2",
    "@types/use-persisted-state": "0.3.0",
    "@typescript-eslint/eslint-plugin": "1.4.0",
    "@wkovacs64/eslint-config-ts-react": "2.0.1",
    "axe-core": "3.1.2",
    "babel-eslint": "10.0.1",
    "cypress": "3.1.5",
    "cypress-axe": "0.3.0",
    "cypress-testing-library": "2.3.6",
    "eslint": "5.14.1",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-emotion": "10.0.7",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.0.2",
    "husky": "1.3.1",
    "lint-staged": "8.1.4",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "start-server-and-test": "1.7.11",
