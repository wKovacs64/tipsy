{
  "name": "tipsy",
  "version": "2.1.1",
  "description": "A simple tip calculator and bill divider PWA",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/tipsy.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/tipsy/issues"
  },
  "scripts": {
    "build": "tsc && vite build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "cy:open": "cypress open --config baseUrl=http://localhost:5173",
    "cy:run": "cypress run --config baseUrl=http://localhost:4173",
    "dev": "vite",
    "format": "prettier --cache --write .",
    "lint": "eslint .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "preview": "vite preview",
    "test": "npm run test:e2e:ci",
    "test:e2e:open": "start-server-and-test dev http-get://localhost:5173 cy:open",
    "test:e2e:run": "start-server-and-test preview http-get://localhost:4173 cy:run"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@babel/core": "7.19.1",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@fontsource/source-sans-pro": "4.5.11",
    "@semantic-release/git": "10.0.1",
    "@testing-library/cypress": "8.0.3",
    "@types/node": "16.11.59",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/react-helmet": "6.1.5",
    "@vitejs/plugin-react": "2.1.0",
    "@wkovacs64/prettier-config": "3.0.2",
    "autoprefixer": "10.4.11",
    "clsx": "1.2.1",
    "commitizen": "4.2.5",
    "currency.js": "2.0.4",
    "cypress": "10.8.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.23.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-wkovacs64": "13.16.0",
    "git-current-commit": "1.1.0",
    "history": "5.3.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.4.0",
    "react-router-dom": "6.4.0",
    "react-switch": "7.0.0",
    "semantic-release": "19.0.5",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.3",
    "use-local-storage-state": "18.1.1",
    "vite": "3.1.2",
    "vite-plugin-pwa": "0.12.7",
    "workbox-build": "6.5.4",
    "workbox-window": "6.5.4"
  }
}
