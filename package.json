{
  "name": "tipsy",
  "version": "1.2.3",
  "description": "A simple tip calculator and bill divider PWA",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/tipsy.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/tipsy/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "check-types": "tsc",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,ts,tsx,html,json,md,yml,yaml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "preserve": "yarn build",
    "serve": "gatsby serve --port 8000",
    "test": "yarn test:e2e:ci",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test serve 8000 cy:run",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.14",
    "@emotion/styled": "10.0.14",
    "@quid/merge-refs": "3.0.0",
    "@wkovacs64/normalize.css": "8.0.1",
    "currency.js": "1.2.2",
    "gatsby": "2.8.8",
    "gatsby-plugin-emotion": "4.0.7",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-netlify": "2.0.17",
    "gatsby-plugin-offline": "2.1.3",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-typescript": "2.0.15",
    "gatsby-plugin-typography": "2.2.13",
    "gatsby-plugin-use-dark-mode": "1.1.2",
    "git-current-commit": "1.1.0",
    "hex-rgb": "4.1.0",
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "6.0.0-beta",
    "react-icons": "3.7.0",
    "react-switch": "5.0.1",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-moraga": "0.16.19",
    "use-dark-mode": "2.3.1",
    "use-persisted-state": "0.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@cypress/browserify-preprocessor": "2.1.1",
    "@semantic-release/git": "7.0.16",
    "@testing-library/cypress": "4.0.4",
    "@types/cypress-axe": "0.4.0",
    "@types/hex-rgb": "3.0.0",
    "@types/lodash": "4.14.136",
    "@types/node": "10.14.13",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "@types/typography": "0.16.2",
    "@types/use-persisted-state": "0.3.0",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@wkovacs64/eslint-config-ts-react": "3.0.0",
    "axe-core": "3.3.0",
    "babel-eslint": "10.0.2",
    "commitizen": "4.0.3",
    "cypress": "3.4.0",
    "cypress-axe": "0.5.1",
    "cz-conventional-changelog": "3.0.1",
    "eslint": "5.16.0",
    "eslint-plugin-cypress": "2.6.0",
    "eslint-plugin-emotion": "10.0.14",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.12.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "husky": "3.0.1",
    "lint-staged": "9.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "semantic-release": "15.13.18",
    "start-server-and-test": "1.9.1",
    "tsify": "4.0.1",
    "typescript": "3.5.3"
  },
  "resolutions": {
    "**/@types/react": "16.8.23"
  }
}
