{
  "name": "tipsy",
  "version": "1.2.5",
  "description": "A simple tip calculator and bill divider PWA",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/tipsy.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/tipsy/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "preserve": "yarn build",
    "serve": "gatsby serve --port 8000",
    "test": "yarn test:e2e:ci",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test serve 8000 cy:run",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@quid/merge-refs": "4.0.0",
    "@wkovacs64/normalize.css": "8.0.3",
    "currency.js": "2.0.3",
    "gatsby": "3.5.0",
    "gatsby-plugin-emotion": "6.5.0",
    "gatsby-plugin-manifest": "3.5.0",
    "gatsby-plugin-netlify": "3.5.0",
    "gatsby-plugin-offline": "4.5.0",
    "gatsby-plugin-react-helmet": "4.5.0",
    "gatsby-plugin-typescript": "3.5.0",
    "gatsby-plugin-typography": "3.5.0",
    "gatsby-plugin-use-dark-mode": "1.3.0",
    "git-current-commit": "1.1.0",
    "hex-rgb": "5.0.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "4.2.0",
    "react-switch": "6.0.0",
    "react-typography": "0.16.20",
    "typography": "0.16.21",
    "typography-theme-moraga": "0.16.19",
    "use-dark-mode": "2.3.1",
    "use-persisted-state": "0.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@semantic-release/git": "9.0.0",
    "@testing-library/cypress": "7.0.6",
    "@types/hex-rgb": "4.1.1",
    "@types/node": "12.20.13",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.5",
    "@types/react-helmet": "6.1.1",
    "@types/typography": "0.16.3",
    "@types/use-persisted-state": "0.3.0",
    "@wkovacs64/prettier-config": "3.0.0",
    "babel-eslint": "10.1.0",
    "babel-preset-gatsby": "1.5.0",
    "commitizen": "4.2.4",
    "core-js": "3.12.1",
    "cypress": "7.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.26.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-wkovacs64": "13.1.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "semantic-release": "17.4.3",
    "start-server-and-test": "1.12.2",
    "typescript": "4.2.4"
  },
  "resolutions": {
    "**/@types/react": "17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0"
  }
}
