  "devDependencies": {
    "@babel/core": "7.2.2",
    "@types/cypress-axe": "0.3.0",
    "@types/node": "10.12.18",
    "@types/react": "16.7.21",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "5.0.8",
    "@types/typography": "0.16.2",
    "@typescript-eslint/eslint-plugin": "1.1.0",
    "@typescript-eslint/parser": "1.1.0",
    "axe-core": "3.1.2",
    "cypress": "3.1.3",
    "cypress-axe": "0.3.0",
    "cypress-testing-library": "2.3.5",
    "eslint": "5.12.1",
